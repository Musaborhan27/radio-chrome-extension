{
  "name": "Groove Radio PT",
  "version": "1.0.0",
  "description": "Listen to the Groove Radio while browsing the web!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js", "jquery-3.3.1.min.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/grooveradio16.png",
      "24": "images/grooveradio24.png",
      "32": "images/grooveradio32.png",
      "48": "images/grooveradio48.png",
      "128": "images/grooveradio128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/grooveradio16.png",
    "24": "images/grooveradio24.png",
    "32": "images/grooveradio32.png",
    "48": "images/grooveradio48.png",
    "128": "images/grooveradio128.png"
    },
    "manifest_version": 2
}
